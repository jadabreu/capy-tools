{
  "name": "open-jungle",
  "version": "1.1.0",
  "description": "Chrome extension for extracting forecast data from Amazon pages",
  "scripts": {
    "build": "webpack --config webpack.config.js && node scripts/build.js",
    "watch": "webpack --watch --config webpack.config.js",
    "package": "webpack --config webpack.config.js && node scripts/build.js --package",
    "postinstall": "npm run build",
    "version": "node scripts/version.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "archiver": "^5.3.1",
    "fs-extra": "^11.1.1",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "babel-loader": "^8.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0"
  }
}
